/* Rofi Config for Google Search) */

@import "~/.config/rofi/config-compact.rasi" 

/* ---- Window ---- */
window {
	width: 50%;
    y-offset: 26px;
    location: north;
}

/* ---- Inputbar ---- */
inputbar {
    enabled: true;
    text-color: @foreground;
}

/* ---- Mainbox ---- */
mainbox {
	padding: 0px;
    children: [ "entry"];
}

/* ---- Entry ---- */ 
